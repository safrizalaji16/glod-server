{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "price": {
      "type": "biginteger",
      "required": true,
      "default": "0"
    },
    "discount": {
      "type": "biginteger"
    },
    "material": {
      "type": "text"
    },
    "productLink": {
      "type": "string",
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)",
      "required": true
    },
    "status": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "jersey",
        "accecories"
      ],
      "required": true
    },
    "colors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::color.color",
      "mappedBy": "product"
    },
    "images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::image.image",
      "mappedBy": "product"
    },
    "sizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::size.size",
      "mappedBy": "product"
    }
  }
}
